+import io.cucumber.java.en.When;
+
+import View.GameFrame;
+
+public class VirologistBearDance {
+    private GameFrame gameFrame;
+    private Virologist virologist;
+    private Virologist secondVirologist;
+    void virologistStepRandomTile(){
+        gameFrame = new GameFrame();
+        virologist = RoundManager.getInstance().getCurrentVirologist();
+        Tile tile = new Tile();
+        virologist.move(tile);
+
+    }
+
+    void infectAnAnotherVirologist(){
+
+        secondVirologist = RoundManager.getInstance().getCurrentVirologist();
+        secondVirologist.move(virologist.getCurrentTile());
+
